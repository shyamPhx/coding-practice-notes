// 1.Define 3 function.
// The first function fun1 return "I am fun 1"
// The first function fun2 return "I am fun 2"
// The first function fun3 return "I am fun 3"
// & display 3 output on console.

function fun1() {
  return "I am function 1";
}

function fun2() {
  return "I am function 2";
}

function fun3() {
  return "I am function 3";
}

console.log(fun1());
console.log(fun2());
console.log(fun3());



// // Write a function check_threedigit .If a number is greater than or equal to 100
// // &  less than 1000
// // It should return boolean value as true or false
// // && and if else
// // 10=>false
// 200=>true

function check_threedigit(value) {
  if (value >= 100 && value < 1000) {
    return true;
  } else {
    return false;
  }
}

console.log(check_threedigit(10000));


// Write a function onedigit which takes natural number and return one digit of
// that number
// input:
// 1357
// ouput:7

function get_one_Digit(num) {
  return num % 10;
}

console.log(get_one_Digit(1357));



// Write a function check_equal which takes 3 number from user for
// strict equality .The function should return boolean value if all 3 values
// are equal
// Input:
// 5,5,5
// Output:true

// Input:
// 5,6,5
// Output:false

function check_equal(num1, num2, num3) {
  return num1 === num2 && num1 === num3;
}

console.log(check_equal(5, 6, 5));



// Predict the value of num after executing below code snippet.

let num = 3;
num++; //4
num = num * 2; //8
num--; //7

console.log(num--); //7
console.log(num); // 6



// Write a function check_unequal which takes 3 number from user for
// strict inequality .The function should return boolean value if all 3 values
// are  strict unequal =>true els false
// 2,3,4=>true

// 4,4,5=false
function check_unequal(num1, num2, num3) {
  return num1 !== num2 && num1 != num3 && num2 !== num3;
}

console.log(check_unequal(4, 4, 5));
